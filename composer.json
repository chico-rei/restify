{
    "name": "chico-rei/restify",
    "description": "Automatic REST APIs for Eloquent Models",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigo Bendia",
            "email": "rodrigo@chicorei.com.br"
        }
    ],
    "require": {
        "laravel/framework": "^5.5|^6|^7",
        "league/fractal": "0.13.*"
    },
    "autoload": {
        "psr-4" : {
            "ChicoRei\\Packages\\" : ["src/"]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChicoRei\\Packages\\Restify\\RouterServiceProvider"
            ],
            "aliases": {
                "Restify": "ChicoRei\\Packages\\Restify\\Facades\\Router"
            }
        }
    }
}
